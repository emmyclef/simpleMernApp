{
  "name": "express-app",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "cd client && npm run build",
    "install-client": "cd client && npm install",
    "heroku-postbuild": "npm run install-client && npm run build",
    "start": "node app",
    "client": "cd client && npm start",
    "dev": "concurrently -n 'server,client' -c 'red,green' \"nodemon app.js\" \"npm run client\""
  },
  "description": "ExpressApp",
  "author": {
    "name": ""
  },
  "dependencies": {
    "axios": "^0.21.1",
    "body-parser": "^1.15.0",
    "concurrently": "^5.3.0",
    "cookie-parser": "^1.4.0",
    "debug": "^2.2.0",
    "express": "^4.14.0",
    "mongoose": "^5.11.13",
    "morgan": "^1.7.0",
    "nodemon": "^2.0.7",
    "pug": "^2.0.0-beta6",
    "serve-favicon": "^2.3.0"
  }
}
